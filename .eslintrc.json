{
    "extends": ["@endouble.com/eslint-config-endouble/react"],
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js", ".jsx"]
            }
        }
    },
    "globals": {
        "window": true,
        "document": true,
        "localStorage": true,
        "navigator": true,
        "fetch": true,
        "describe": true,
        "it": true,
        "expect": true,
        "beforeEach": true,
        "afterEach": true,
        "Headers": true,
        "jest": true
    }
}
